# Environment variables for Certificate System
# Copy this file to `.env` and fill in values

# Server port
PORT=3001

# Session secret for express-session
SESSION_SECRET=your-long-random-session-secret

# MongoDB connection string
# For local MongoDB (requires MongoDB service running locally):
# MONGODB_URI=mongodb://localhost:27017/simple_certificates
# For MongoDB Atlas (replace <username>, <password>, and <cluster-host>):
# MONGODB_URI=mongodb+srv://<username>:<password>@<cluster-host>/simple_certificates?retryWrites=true&w=majority
MONGODB_URI=
